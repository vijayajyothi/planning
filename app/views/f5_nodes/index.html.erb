<h1>Listing f5_nodes</h1>

<table>
  <tr>
    <th>Ip</th>
    <th>Port</th>
    <th>F5 pool</th>
    <th>F5 cluster</th>
    <th>Vm</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @f5_nodes.each do |f5_node| %>
  <tr>
    <td><%= f5_node.ip %></td>
    <td><%= f5_node.port %></td>
    <td><%= f5_node.f5_pool_id %></td>
    <td><%= f5_node.f5_cluster_id %></td>
    <td><%= f5_node.vm_id %></td>
    <td><%= link_to 'Show', f5_node %></td>
    <td><%= link_to 'Edit', edit_f5_node_path(f5_node) %></td>
    <td><%= link_to 'Destroy', f5_node, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

