    <th>Vm hostname</th>
    <th>Ip</th>
    <th>Vmhost</th>
    <th>Tier</th>
    <th>Backup</th>
    <th>Hw version</th>
    <th>Total mem mb</th>
    <th>Num cpus</th>
    <th>Num vnics</th>
    <th>Power state</th>
