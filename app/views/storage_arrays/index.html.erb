<h1>Listing storage_arrays</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Ip</th>
    <th>Uid</th>
    <th>Model</th>
    <th>Model type</th>
    <th>Agent revision</th>
    <th>Prom revision</th>
    <th>Revision</th>
    <th>Serial no</th>
    <th>Cabinet</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @storage_arrays.each do |storage_array| %>
  <tr>
    <td><%= storage_array.name %></td>
    <td><%= storage_array.ip %></td>
    <td><%= storage_array.uid %></td>
    <td><%= storage_array.model %></td>
    <td><%= storage_array.model_type %></td>
    <td><%= storage_array.agent_revision %></td>
    <td><%= storage_array.prom_revision %></td>
    <td><%= storage_array.revision %></td>
    <td><%= storage_array.serial_no %></td>
    <td><%= storage_array.cabinet %></td>
    <td><%= link_to 'Show', storage_array %></td>
    <td><%= link_to 'Edit', edit_storage_array_path(storage_array) %></td>
    <td><%= link_to 'Destroy', storage_array, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

